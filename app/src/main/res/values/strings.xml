<?xml version="1.0" encoding="UTF-8"?>

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

        <string name="app_name">Weather</string>
        <string name="action_settings">Settings</string>
        <string name="search">Search</string>
        <string name="title_activity_maps">Map</string>

    <string name="format_info">
        Last update: <xliff:g id="time">%1$s</xliff:g>
        <xliff:g id="city">%2$s,</xliff:g>
        <xliff:g id="country">%3$s</xliff:g>
    </string>

    <string name="format_temperature">
        <xliff:g id="temp">%1.1f</xliff:g>
        <xliff:g id="degree_sign">\u00B0</xliff:g>
    </string>

    <string name="format_maxmin">
        High:<xliff:g id="tempH">%1.0f</xliff:g><xliff:g id="deg1">\u00B0</xliff:g>
        / Low:<xliff:g id="tempL">%2.0f</xliff:g><xliff:g id="deg2">\u00B0</xliff:g>
    </string>


    <string name="format_wind">
        Wind: <xliff:g id="speed">%1.0f</xliff:g>m/s <xliff:g id="direction">%2$s</xliff:g>
    </string>

    <string name="format_pressure">
        Pressure:<xliff:g id="press">%1.0f</xliff:g> hPa
    </string>

    <string name="format_humidity">
        Humidity:<xliff:g id="humid">%d</xliff:g>%%
    </string>

    <string name="format_sunrise">
        Sunrise:<xliff:g id="sunUpForm">%1$s</xliff:g>
    </string>

    <string name="format_sunset">
        Sunset:<xliff:g id="sunDnForm">%1$s</xliff:g>
    </string>

    <string name="format_clouds">
        Clouds:<xliff:g id="sunDnForm">%d</xliff:g>%%
    </string>

    <string name="format_seaLvl">
        Sea level:<xliff:g id="sea">%1$s</xliff:g> hPa
    </string>

    <string name="format_grndLvl">
        Ground level:<xliff:g id="grnd">%1$s</xliff:g> hPa
    </string>

</resources>
